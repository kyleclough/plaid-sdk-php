{
    "name": "kyleclough/plaid-sdk-php",
    "description": "PHP implementation of Plaid's AP as an SDK.",
    "keywords": [
        "plaid",
        "sdk",
        "php"
    ],
    "type": "library",
    "require": {
        "php": ">=7.3 || >=8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "nimbly/shuttle": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "KyleClough\\Plaid\\" : "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.0",
        "symfony/var-dumper": "^4.2",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "KyleClough\\Plaid\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Clough",
            "email": "kyle@kyleclough.com"
        }
    ]
}
